#include<stdio.h>

int main()
{
int grade;
printf("Enter your grade here: ");
scanf("%d,&grade);

if(100<=grade && grade<=85)
{
printf("Grade A");
}

else if("84<=grade && grade<=70)
{
printf("Grade B");
}

else if("69<=grade && grade<=55)
{
printf("Grade C");
}

else if(54<=grade && grade<=40)
{
printf("Grade D");
}

else
{
printf("Grade F");
}

return 0;
}
